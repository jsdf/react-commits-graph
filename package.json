{
  "name": "react-commits-graph",
  "version": "0.2.0",
  "description": "a react component to render an svg graph of git commits",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "mkdir -p lib/ && rm lib/*.js && npm run build",
    "build": "coffee -cbo lib/ src/*.coffee",
    "watch": "coffee -cbwo lib/ src/*.coffee",
    "demo": "examples/run.js",
    "demo-watch": "examples/run.js watch"
  },
  "keywords": [
    "react-component",
    "react",
    "git",
    "graph",
    "visualization"
  ],
  "author": "James Friend",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/jsdf/react-commits-graph/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/jsdf/react-commits-graph",
  "bugs": "https://github.com/jsdf/react-commits-graph/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/jsdf/react-commits-graph.git"
  },
  "devDependencies": {
    "coffee-script": "^1.9.0",
    "react": "^0.12.2"
  }
}
